{
  "manifest_version": 2,
  "name": "Xnl Reveal",
  "description": "Show alerts for reflected query params; Show Wayback endpoints; Show hidden elements; Enable disabled elements. See https://github.com/xnl-h4ck3r/XnlReveal for more details.",
  "version": "2.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "contextMenus",
    "webNavigation",
    "<all_urls>"
  ],
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@xnl.reveal",
      "strict_min_version": "1.0"
    }
  }
}